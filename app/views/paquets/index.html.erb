<h1>Listing paquets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paquets.each do |paquet| %>
      <tr>
        <td><%= link_to 'Show', paquet %></td>
        <td><%= link_to 'Edit', edit_paquet_path(paquet) %></td>
        <td><%= link_to 'Destroy', paquet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paquet', new_paquet_path %>
