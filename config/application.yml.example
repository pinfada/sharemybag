# Figaro environment variables template
# Copy this file to config/application.yml and fill in the values
# NEVER commit config/application.yml to version control

# Database
DB_USERNAME: "ubuntu"
DB_PASSWORD: "your_secure_password_here"
DB_NAME_DEV: "sharemybag_development"
DB_NAME_TEST: "sharemybag_test"
DB_NAME_PROD: "sharemybag_production"
